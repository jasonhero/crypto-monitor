{
    "name": "crypto-monitor",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "source ./config/dev-env.sh && concurrently -k -p \"[{name}]\" \"npm run webpack-server\" \"npm run build && npm run watch\"",
        "build": "npm run build-ts && npm run tslint",
        "webpack-server": "webpack-dev-server --progress --watch",
        "serve": "nodemon dist/server/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
        "test": "jest --forceExit",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "migrate": "source ./config/dev-env.sh && sequelize db:migrate --config=config/sequelize.config.js --migrations-path=server/db/migrations --env development",
        "migrations:rollback": "source ./config/dev-env.sh && knex migrate:rollback"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jasonhero/crypto-monitor.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/jasonhero/crypto-monitor/issues"
    },
    "homepage": "https://github.com/jasonhero/crypto-monitor#readme",
    "dependencies": {
        "axios": "^0.16.2",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-polyfill": "^6.26.0",
        "body-parser": "^1.18.2",
        "color": "^2.0.1",
        "cors": "^2.8.4",
        "epilogue": "^0.7.1",
        "express": "^4.15.4",
        "html-webpack-plugin": "^2.30.1",
        "http-proxy-middleware": "^0.17.4",
        "lodash": "^4.17.4",
        "material-ui": "^1.0.0-beta.22",
        "moment": "^2.19.2",
        "moment-timezone": "^0.5.14",
        "pg": "^7.4.0",
        "pg-hstore": "^2.3.2",
        "react": "^16.1.1",
        "react-dom": "^16.1.1",
        "react-hot-loader": "^3.1.3",
        "react-redux": "^5.0.6",
        "react-redux-form": "^1.16.2",
        "react-router-dom": "^4.2.2",
        "redux": "^3.7.2",
        "redux-observable": "^0.17.0",
